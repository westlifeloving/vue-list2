{"ast":null,"code":"import Header from './components/layout/Header.vue';\nimport Todos from './components/Todos.vue';\nimport AddTodo from './components/AddTodo.vue';\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Todos,\n    AddTodo\n  },\n\n  data() {\n    return {\n      todos: [{\n        title: \"写代码\",\n        completed: false\n      }, {\n        title: \"看书\",\n        completed: false\n      }, {\n        title: \"玩游戏\",\n        completed: true\n      }, {\n        title: \"看电视\",\n        completed: false\n      }, {\n        title: \"休息一会吧\",\n        completed: false\n      }, {\n        title: \"继续学习\",\n        completed: false\n      }]\n    };\n  },\n\n  methods: {\n    deleteTodo(id) {\n      this.todos = this.todos.filter(todo => todo.id !== id);\n    },\n\n    AddTodo(newTodo) {\n      this.todos = [...this.todos, newTodo];\n    }\n\n  }\n};","map":{"version":3,"mappings":"AASA;AACA;AACA;AAEA;EACAA,WADA;EAEAC;IACAC,MADA;IAEAC,KAFA;IAGAC;EAHA,CAFA;;EAOAC;IACA;MACAC,QACA;QACAC,YADA;QAEAC;MAFA,CADA,EAKA;QACAD,WADA;QAEAC;MAFA,CALA,EASA;QACAD,YADA;QAEAC;MAFA,CATA,EAaA;QACAD,YADA;QAEAC;MAFA,CAbA,EAiBA;QACAD,cADA;QAEAC;MAFA,CAjBA,EAqBA;QACAD,aADA;QAEAC;MAFA,CArBA;IADA;EA4BA,CApCA;;EAqCAC;IACAC;MACA;IACA,CAHA;;IAIAN;MACA;IACA;;EANA;AArCA","names":["name","components","Header","Todos","AddTodo","data","todos","title","completed","methods","deleteTodo"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <Header />\n    <AddTodo v-on:add-todo=\"addTodo\"/>\n    <Todos v-bind:todos=\"todos\" v-on:del-todo=\"deleteTodo\"/>\n  </div>\n</template>\n\n<script>\nimport Header from './components/layout/Header.vue';\nimport Todos from './components/Todos.vue';\nimport AddTodo from './components/AddTodo.vue';\n\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Todos,\n    AddTodo\n  },\n  data(){\n    return{\n      todos:[\n        {\n          title:\"写代码\",\n          completed: false\n        },\n        {\n          title:\"看书\",\n          completed: false\n        },\n        {\n          title:\"玩游戏\",\n          completed: true\n        },\n                {\n          title:\"看电视\",\n          completed: false\n        },\n        {\n          title:\"休息一会吧\",\n          completed: false\n        },\n        {\n          title:\"继续学习\",\n          completed: false\n        },\n      ]\n    }\n  },\n  methods:{\n    deleteTodo(id){\n      this.todos = this.todos.filter(todo => todo.id !== id);\n    },\n    AddTodo(newTodo){\n      this.todos = [...this.todos, newTodo];\n    }\n  }\n}\n</script>\n\n<style>\n *{\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n }\n\n body{\n  font-family: Arial, Helvetica, sans-serif;\n  line-height: 1.4;\n }\n\n .btn{\n  display: inline-block;\n  border: none;\n  background: #555;\n  color: #fff;\n  padding: 7px 20px;\n  cursor: pointer;\n }\n\n .btn:hover{\n  background: #666;\n }\n</style>\n"]},"metadata":{},"sourceType":"module"}