{"ast":null,"code":"import Header from './components/layout/Header.vue';\nimport Todos from './components/Todos.vue';\nimport AddTodo from './components/AddTodo.vue';\nimport SearchTodo from './components/SearchTodo.vue';\nimport axios from 'axios';\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Todos,\n    AddTodo,\n    SearchTodo\n  },\n\n  data() {\n    return {\n      todos: []\n    };\n  },\n\n  methods: {\n    deleteTodo(id) {\n      this.todos = this.todos.filter(todo => todo.id !== id);\n    },\n\n    addTodo(newTodo) {\n      this.todos = [...this.todos, newTodo];\n    }\n\n  },\n\n  created() {\n    axios.get('https://jsonplaceholder.typicode.com/todos').then(res => this.todos = res.data).catch(err => console.log(err));\n  },\n\n  computed: {\n    filteredTodo: function () {\n      return this.todos.filter(todo => {\n        return todo.title.match(this.search);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAUA;AACA;AACA;AACA;AACA;AAEA;EACAA,WADA;EAEAC;IACAC,MADA;IAEAC,KAFA;IAGAC,OAHA;IAIAC;EAJA,CAFA;;EAQAC;IACA;MACAC;IADA;EAGA,CAZA;;EAaAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACA;IACA;;EANA,CAbA;;EAqBAC;IACAC,wDACAC,IADA,CACAC,4BADA,EAEAC,KAFA,CAEAC,uBAFA;EAGA,CAzBA;;EA0BAC;IACAC;MACA;QACA;MACA,CAFA;IAGA;EALA;AA1BA","names":["name","components","Header","Todos","AddTodo","SearchTodo","data","todos","methods","deleteTodo","addTodo","created","axios","then","res","catch","err","computed","filteredTodo"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <Header />\n    <AddTodo v-on:add-todo=\"addTodo\"/>\n    <SearchTodo v-on:search-todo=\"filteredTodo\"/>\n    <Todos v-bind:todos=\"todos\" v-on:del-todo=\"deleteTodo\"/>\n  </div>\n</template>\n\n<script>\nimport Header from './components/layout/Header.vue';\nimport Todos from './components/Todos.vue';\nimport AddTodo from './components/AddTodo.vue';\nimport SearchTodo from './components/SearchTodo.vue';\nimport axios from 'axios';\n\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Todos,\n    AddTodo,\n    SearchTodo\n  },\n  data(){\n    return{\n      todos:[]\n    }\n  },\n  methods:{\n    deleteTodo(id){\n      this.todos = this.todos.filter(todo => todo.id !== id);\n    },\n    addTodo(newTodo){\n      this.todos = [...this.todos, newTodo];\n    }\n  },\n  created(){\n    axios.get('https://jsonplaceholder.typicode.com/todos')\n      .then(res => this.todos = res.data)\n      .catch(err => console.log(err));\n  },\n  computed: {\n    filteredTodo:function() {\n      return this.todos.filter(todo => {\n        return todo.title.match(this.search);\n        });\n    }\n  }\n}\n</script>\n\n<style>\n *{\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n }\n\n body{\n  font-family: Arial, Helvetica, sans-serif;\n  font-size: 14px;\n  line-height: 2;\n }\n\n .btn{\n  display: inline-block;\n  border: none;\n  background: #dc9090;\n  color: #333;\n  padding: 7px 20px;\n  cursor: pointer;\n }\n\n .btn:hover{\n  background: #eee;\n }\n</style>\n"]},"metadata":{},"sourceType":"module"}