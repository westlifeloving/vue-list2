{"ast":null,"code":"import Header from './components/layout/Header.vue';\nimport Todos from './components/Todos.vue';\nimport AddTodo from './components/AddTodo.vue';\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Todos,\n    AddTodo\n  },\n\n  data() {\n    return {\n      todos: [{\n        id: 1,\n        title: \"写作业\",\n        completed: false\n      }, {\n        id: 2,\n        title: \"玩游戏\",\n        completed: true\n      }, {\n        id: 3,\n        title: \"做家务\",\n        completed: false\n      }]\n    };\n  },\n\n  methods: {\n    deleteTodo(id) {\n      this.todos = this.todos.filter(todo => todo.id !== id);\n    },\n\n    AddTodo(newTodo) {\n      this.todos = [...this.todos, newTodo];\n    }\n\n  }\n};","map":{"version":3,"mappings":"AASA;AACA;AACA;AAEA;EACAA,WADA;EAEAC;IACAC,MADA;IAEAC,KAFA;IAGAC;EAHA,CAFA;;EAOAC;IACA;MACAC,QACA;QACAC,KADA;QAEAC,YAFA;QAGAC;MAHA,CADA,EAMA;QACAF,KADA;QAEAC,YAFA;QAGAC;MAHA,CANA,EAWA;QACAF,KADA;QAEAC,YAFA;QAGAC;MAHA,CAXA;IADA;EAmBA,CA3BA;;EA4BAC;IACAC;MACA;IACA,CAHA;;IAIAP;MACA;IACA;;EANA;AA5BA","names":["name","components","Header","Todos","AddTodo","data","todos","id","title","completed","methods","deleteTodo"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <Header />\n    <AddTodo v-on:add-todo=\"addTodo\"/>\n    <Todos v-bind:todos=\"todos\" v-on:del-todo=\"deleteTodo\"/>\n  </div>\n</template>\n\n<script>\nimport Header from './components/layout/Header.vue';\nimport Todos from './components/Todos.vue';\nimport AddTodo from './components/AddTodo.vue';\n\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Todos,\n    AddTodo\n  },\n  data(){\n    return{\n      todos:[\n        {\n          id:1,\n          title:\"写作业\",\n          completed: false\n        },\n        {\n          id:2,\n          title:\"玩游戏\",\n          completed: true\n        },\n        {\n          id:3,\n          title:\"做家务\",\n          completed: false\n        },\n      ]\n    }\n  },\n  methods:{\n    deleteTodo(id){\n      this.todos = this.todos.filter(todo => todo.id !== id);\n    },\n    AddTodo(newTodo){\n      this.todos = [...this.todos, newTodo];\n    }\n  }\n}\n</script>\n\n<style>\n *{\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n }\n\n body{\n  font-family: Arial, Helvetica, sans-serif;\n  line-height: 1.4;\n }\n\n .btn{\n  display: inline-block;\n  border: none;\n  background: #555;\n  color: #fff;\n  padding: 7px 20px;\n  cursor: pointer;\n }\n\n .btn:hover{\n  background: #666;\n }\n</style>\n"]},"metadata":{},"sourceType":"module"}